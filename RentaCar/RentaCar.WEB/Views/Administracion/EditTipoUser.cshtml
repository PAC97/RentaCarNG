
<div  class="ml-3 jumbotron">
    
    <div class="row">
      
        <div class="col-md-8 mt-2">
            Editar Tipo de Usuario
            <form ng-submit="guardarTipoUser">
                <div ng-hide="creando" ng-show="eliminando">
                    ID:
                    <input type="hidden" class="form-control" ng-model="TipoUser.data.TipoUsuarioID" readonly="readonly" />
                </div>

                <input style="width:200px"  type="text" class="form-control mt-3" ng-model="TipoUser.data.Nombre" />
                <select style="width:200px" ng-options="e.EstadoID as e.Nombre for e in EstadoData" ng-model="TipoUser.data.EstadoID" ng-hide="Estadoo" class="form-control mt-3">
                    <option value="">
                        
                    </option>
                </select>
                <input type="submit" ng-click="guardarTipoUser()" value="Guardar" class="btn btn-primary mt-3 mb-3" />
                {{TipoUser.data | json}}
            </form>
        </div>
        <div class="col-md-2"></div>
    </div>
    
</div>


