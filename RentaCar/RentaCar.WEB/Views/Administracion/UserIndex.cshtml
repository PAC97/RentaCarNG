<div class="ml-3">
    Bienvenido: @Session["NombreCompleto"]
    <a href="#!EditUser/nuevo" class="btn btn-success mb-3 mt-2">Crear Nuevo</a>
    <div class="row">
        <div class="col-md-4">
            <input class="form-control" type="text" ng-model="busqueda" placeholder="Buscar Usuario" />
        </div>
    </div>
    <br />
    <table class="table table-striped mb-3">
        <thead>
            <tr>
                <th>#</th>
                <th>Codigo</th>
                <th>Tipo de Usuario</th>
                <th>Nombre</th>
                <th>Apellido</th>
                <th>Email</th>
                <th>Estado</th>
            </tr>
        </thead>
        <tbody class="mb-3">
            <tr ng-repeat="u in UserData | filter:busqueda | limitTo:posicion | limitTo:-3">
                <td>{{numero}}</td>
                <td>{{u.UsuarioID}}</td>
                <td>{{u.TipoUsuario.Nombre}}</td>
                <td>{{u.Nombre}}</td>
                <td>{{u.Apellido}}</td>
                <td>{{u.Email}}</td>
                <td>{{u.Estado.Nombre}}</td>
                <td><a class="btn btn-primary" href="#!EditUser/{{u.UsuarioID}}">Editar</a> | <a class="btn btn-danger" href="#!DeleteUser/{{u.UsuarioID}}">Eliminar</a></td>

            </tr>
        </tbody>
    </table>
    <button class="btn btn-primary" ng-click="anterior()">Anterior</button>
    <button class="btn btn-primary" ng-click="siguiente()">Siguiente</button>
</div>
